{
  "name": "gregfagan.com",
  "version": "0.3.1",
  "description": "A personal homepage",
  "author": "Greg Fagan",
  "homepage": "https://gregfagan.github.io",
  "scripts": {
    "start": "run-p watch serve",
    "watch": "nodemon --ignore *.html --exec 'npx bajel'",
    "serve": "serve build",
    "clean": "rimraf build",
    "build": "bajel",
    "prepublish": "run-s clean build",
    "publish": "gh-pages -b master -s \"**/!(*.js)\" -d build"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.18.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "bajel": "^1.1.0",
    "gh-pages": "^4.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2"
  }
}
